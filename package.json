{
  "name": "webpack-Elitesecutity",
  "version": "1.0.5-RC3",
  "description": "A webpack starter elit",
  "scripts": {
    "start": "webpack-dev-server --inline --port 8080",
    "test": "karma start",
    "json": "json-server json/db.json",
    "build": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail",
    "build:dev": "webpack --config config/webpack.dev.js --progress --profile --colors --display-error-details ",
    "watch:dev": "npm run build:dev -- --watch",
    "postinstall": "typings install"
  },
  "license": "MIT",
